    "postcss-px2rem": "^0.3.0",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-parallel-uglify-plugin": "^0.4.0"
