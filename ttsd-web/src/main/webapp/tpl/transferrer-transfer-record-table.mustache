<table class="table-striped transfer-list">
    <thead>
    <tr>
        <th>转让项目名称</th>
        <th>转让价格(元)</th>
        <th>项目本金(元)</th>
        <th>预期年化收益</th>
        <th>剩余天数</th>
        <th>完成时间</th>
        <th>转让状态</th>
    </tr>
    </thead>
    <tbody>
        {{#records}}
            <tr>
                <td>
                    <a href="/transfer/{{transferApplicationId}}" class="project-name">{{name}}</a>
                </td>
                <td>{{transferAmount}}</td>
                <td>{{investAmount}}</td>
                <td>{{sumRate}}%</td>
                <td>{{leftDays}}</td>
                <td>
                    {{#transferTime}}
                        {{transferTime}}
                    {{/transferTime}}
                    {{^transferTime}} -- {{/transferTime}}
                </td>
                <td>
                    {{transferStatus}}
                </td>
            </tr>
        {{/records}}

        {{^records}}
            <td colspan="7" class="no-data">暂时没有可转让债权记录</td>
        {{/records}}
    </tbody>
</table>

