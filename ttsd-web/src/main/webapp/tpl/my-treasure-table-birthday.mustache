<table class="invest-list-red table-striped">
    <thead>
    <tr>
        <th>项目名称</th>
        <th>首月翻倍倍数</th>
        <th>使用时间</th>
        <th>投资金额(元)</th>
    </tr>
    </thead>
    <tbody>
    {{#records}}
    <tr>
        <td><a href="/loan/{{loanId}}" class="project-name">{{loanName}}</td>
        <td>{{birthdayBenefit}}</td>
        <td>{{usedTime}}</td>
        <td>{{investAmountStr}}</td>
    </tr>
    {{/records}}
    {{^records}}
    <td colspan="4" class="no-data">暂时没有使用记录</td>
    {{/records}}
    </tbody>
</table>