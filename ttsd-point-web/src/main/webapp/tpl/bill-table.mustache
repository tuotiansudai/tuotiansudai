<table class="bill-list table-striped">
    <thead>
        <tr>
            <th>时间</th>
            <th>行为</th>
            <th>积分</th>
            <th>备注</th>
        </tr>
    </thead>
    <tbody>
       {{#records}}
        <tr>
            <td>{{createdTime}}</td>
            <td>{{businessType}}</td>
            <td>{{point}}</td>
            <td>{{note}}</td>
        </tr>
        {{/records}}

        {{^records}}
         <td colspan="4" class="no-data">暂时没有积分记录</td>
        {{/records}}
    </tbody>
</table>

